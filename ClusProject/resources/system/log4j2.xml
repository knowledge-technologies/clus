<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%msg%n" />
		</Console>
		<File name="ClusMainLogFile" fileName="all.log" immediateFlush="false"
			append="false">
			<PatternLayout pattern="%d{yyy-MM-dd HH:mm:ss.SSS} %-5level: %msg%n" />
		</File>
	</Appenders>
	<Loggers>
		<Root level="trace">
			<AppenderRef ref="ClusMainLogFile" />
		</Root>
		<Logger name="CONSOLE" level="debug">
			<AppenderRef ref="Console" />
		</Logger>
	</Loggers>
</Configuration>
